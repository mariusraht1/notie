{
  "name": "notie",
  "version": "08.04.2021-001",
  "description": "Table-based note-taking app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . notie --overwrite --platform=darwin --arch=x64 --icon=res/img/icon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . notie --overwrite --asar --platform=win32 --arch=x64 --icon=res/img/icon.ico --prune=true --out=release-builds --version-string.ProductName=\"Notie\"",
    "package-linux": "electron-packager . notie --overwrite --asar --platform=linux --arch=x64 --icon=res/img/icon.ico --prune=true --out=release-builds"
  },
  "repository": "https://github.com/mariusraht1/notie.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Marius Raht",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "better-sqlite3": "^7.1.5",
    "crypto-js": "^4.0.0",
    "electron-rebuild": "^2.3.5",
    "jquery": "^3.6.0",
    "uuid": "^8.3.2"
  }
}
